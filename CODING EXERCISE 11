package Exercise;

import java.util.Scanner;

public class Exercise11 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		Scanner input = new Scanner (System.in);
		
		int mark;
		System.out.println("Enter percentage marks = ");
		mark = input.nextInt();
		
		switch (mark/10) {
		
		case 9:
			System.out.print("Excellent: Grade A");
			break;
		case 8:
			System.out.println("Very Good Grade B");
			break;
		case 7:
			System.out.println("Good: Grade C");
			break;
		case 6:
			System.out.println("Good: Grade D");
			break;
		case 5:
			System.out.println("Good: Grade E");
			break;
		case 4:
			System.out.println("Good: Grade F");
			break;
		default:
			System.out.println("Failed!");
		}
	}

}
